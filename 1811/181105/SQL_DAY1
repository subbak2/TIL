SELECT LOC, DEPTNO, DNAME
FROM   DEPT;

SELECT  *
FROM    DEPT;

SELECT  ENAME, DEPTNO, HIREDATE
FROM    EMP;

SELECT  ENAME, DEPTNO, SAL, SAL*12
FROM    EMP;

SELECT  ENAME, DEPTNO, SAL, COMM, (SAL*12)+COMM
FROM    EMP;

SELECT  ENAME, DEPTNO, SAL, COMM, (SAL*12)+NVL(COMM,0)
FROM    EMP;

SELECT  ENAME, DEPTNO, SAL, COMM, (SAL*12)+NVL(COMM,0)  실연봉
FROM    EMP;

SELECT  ENAME, DEPTNO, SAL, COMM, (SAL*12)+NVL(COMM,0) AS "1 year"
FROM    EMP;

SELECT  EMPNO, ENAME||' 사원' , '$'||SAL
FROM    EMP;

SELECT  DISTINCT DEPTNO
FROM    EMP;

SELECT  DISTINCT DEPTNO, JOB
FROM    EMP;

SELECT  *
FROM    EMP
WHERE   DEPTNO = 10;

SELECT  *
FROM    EMP
WHERE   DEPTNO <> 10;
--WHERE   DEPTNO != 10;

SELECT  * 
FROM    EMP
WHERE   ENAME = 'smith';

SELECT  *
FROM    EMP
WHERE   SAL >= 2000;

SELECT  *
FROM    EMP
WHERE   SAL BETWEEN 2000 AND 3000;

--SELECT  *
--FROM    EMP
--WHERE   SAL BETWEEN 3000 AND 2000;

SELECT  *
FROM    EMP
WHERE   SAL >= 2000 
AND     SAL <= 3000;

SELECT  *
FROM    EMP
WHERE   DEPTNO = 10
OR      DEPTNO = 20;

SELECT  *
FROM    EMP
WHERE   DEPTNO IN(10, 20);

SELECT  *
FROM    EMP
WHERE   not SAL BETWEEN 2000 AND 3000;

SELECT  *
FROM    EMP
WHERE   DEPTNO not IN(10, 20);

SELECT  *
FROM    EMP
WHERE   COMM IS NULL;

SELECT  *
FROM    EMP
WHERE   COMM IS NOT NULL;

--   _:1개의 임의의 문자
--   %:0개이상의 임의의 문자
SELECT  *
FROM    EMP
WHERE   ENAME LIKE '_A__';
--WHERE   ENAME LIKE '_A%';
--WHERE   ENAME LIKE '%A%';
--WHERE   ENAME LIKE '%S';
--WHERE   ENAME LIKE 'S%';

SELECT  EMPNO,DEPTNO,SAL
FROM    EMP
WHERE   DEPTNO = 10
OR      DEPTNO = 20
AND     SAL > 2000;

SELECT  EMPNO,DEPTNO,SAL
FROM    EMP
WHERE   DEPTNO = 10
OR      (DEPTNO = 20
AND     SAL > 2000);



SELECT  EMPNO,DEPTNO,SAL
FROM    EMP
WHERE   (DEPTNO = 10
OR      DEPTNO = 20)
AND     SAL > 2000;

SELECT  EMPNO,DEPTNO,SAL
FROM    EMP
WHERE   DEPTNO IN( 10 ,20)
AND     SAL > 2000;

SELECT   EMPNO,ENAME,SAL,HIREDATE AS HDATE
FROM     EMP
ORDER BY 4 DESC;
--ORDER BY HDATE;
--ORDER BY HIREDATE;


SELECT   EMPNO,ENAME,DEPTNO,SAL,HIREDATE AS HDATE
FROM     EMP
ORDER BY DEPTNO,SAL DESC;

SELECT      EMPNO,COMM
FROM        EMP
ORDER BY    COMM DESC NULLS LAST;





